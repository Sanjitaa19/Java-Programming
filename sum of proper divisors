import java.util.*;

public class Hello {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();  // Read input
        int sum = 0;

        for (int i = 1; i <= n; i++) {  // Loop should go up to `n`
            if (n % i == 0) {  // Check if `i` is a divisor
                sum += i;  // Add divisor to sum
            }
        }

        System.out.print(sum);  // Print sum of divisors
    }
}

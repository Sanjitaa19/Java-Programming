import java.util.*;

public class Hello {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        String s = sc.nextLine();
        char ch = sc.next().charAt(0);
        int n = s.length();

        for (int i = 0; i < n; i++) {
            if (s.charAt(i) == ch) {
                System.out.println(s.substring(0, i));
                return;
            }
        }

        System.out.println(s);
    }
}
